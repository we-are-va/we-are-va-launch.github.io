            ul.rd-navbar-dropdown
              li: a(href="404.html") 404
              li: a(href="coming.html") Coming soon
              li: a(href="login.html") Login page
              li: a(href="maintenance.html") Maintenance page
              li: a(href="register.html") Register page
              li: a(href="search-res.html") Search results page
              li: a(href="terms.html") Terms of Service

          li(class=(active == 'Pages' ? 'active' : '')).dropdown-megamenu
