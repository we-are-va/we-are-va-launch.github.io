                  li: a(href="index.html") Header Transparent
                  li: a(href="index-cent.html") Header Centre, Footer Light
                  li: a(href="index-min.html") Header Minimal, Footer Light
                  li: a(href="index-topbar.html") Header Corporate
                  li: a(href="index-sidebar.html") Header Hamburger Menu
                  li: a(href="index-cent-dark.html") Footer Centre Dark
                  li: a(href="index-min-dark.html") Footer Minimal Dark
                  li: a(href="index-wid.html") Footer Widget Light
                  li: a(href="index-wid-dark.html") Footer Widget Dark

              li
                p E-commerce
                ul
                  li: a(href="shop-catalog.html") Product catalog
                  li: a(href="shop-product.html") Single product
                  li: a(href="shop-cart.html") Shopping cart
                  li: a(href="shop-checkout.html") Checkout
            //END RD Navbar Megamenu

          li(class=(active == 'Blog' ? 'active' : ''))
            a(href="#") Blog
            ul.rd-navbar-dropdown
              li: a(href="blog_default.html") Default Blog
              li: a(href="blog_columns_2.html") 2 Columns Blog
              li: a(href="blog_columns_3.html") 3 Columns Blog
              li: a(href="blog_archive.html") Archive page
              li: a(href="blog_post.html") Post page
              li: a(href="timeline.html") Timeline #[span.marked hot]

          li(class=(active == 'Gallery' ? 'active' : ''))
            a(href="#") Gallery
